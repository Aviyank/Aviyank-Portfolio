{
  "version": 2,
  "builds": [
    {
      "src": "backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/backend/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/backend/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
